{
  "name": "examples",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npm run loadstories && react-native run-android",
    "ios": "npm run loadstories && react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "loadstories": "rnstl"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/stories"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "dependencies": {
    "harmonium-rn": "file:../harmonium-rn-1.0.0.tgz",
    "react": "16.13.1",
    "react-native": "0.63.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react-native": "^5.3.21",
    "@storybook/react-native-server": "^5.3.21",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "16.13.1",
    "react-native-storybook-loader": "^2.0.2",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
